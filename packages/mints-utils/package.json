{
  "name": "mints-utils",
  "version": "1.0.0-alpha.1",
  "description": "utils package for mints",
  "keywords": [
    "mints",
    "utils"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel ./src --out-dir ./lib",
    "publish": "npm run build && npm run test && npm run publish"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html"
    ],
    "testEnvironment": "jest-environment-jsdom-global"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "jest-environment-jsdom": "^24.8.0",
    "jest-environment-jsdom-global": "^1.2.0"
  }
}

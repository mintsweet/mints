{
  "name": "mints-utils",
  "version": "1.0.0-beta.3",
  "description": "utils package for mints",
  "keywords": [
    "mints",
    "utils"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm test && npm build"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0"
  }
}
